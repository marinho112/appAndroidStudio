<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="70dp"
    tools:context=".componentes.objeto_3del5_item_lista_caracteristicas">

    <TextView
        android:id="@+id/objeto_3del5_item_lista_caracteristicas_textNome"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_margin="8dp"
        android:textSize="20sp"
        android:text="Insira o Nome Aqui"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/objeto_3del5_item_lista_caracteristicas_textCusto"/>

    <TextView
        android:id="@+id/objeto_3del5_item_lista_caracteristicas_textCusto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:textSize="20sp"
        android:text="0pts"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/objeto_3del5_item_lista_caracteristicas_textNome"
        app:layout_constraintTop_toTopOf="@id/objeto_3del5_item_lista_caracteristicas_textNome"
        app:layout_constraintStart_toEndOf="@+id/objeto_3del5_item_lista_caracteristicas_textNome" />
</android.support.constraint.ConstraintLayout>