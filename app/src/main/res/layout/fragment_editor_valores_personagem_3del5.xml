        android:textSize="@dimen/fonte_tableLayout_atributos_nome_atributos"
        android:text="+"
        app:layout_constraintTop_toTopOf="@id/editText_soma_editor_valores_3del5"
        app:layout_constraintBottom_toBottomOf="@id/editText_soma_editor_valores_3del5"
        app:layout_constraintStart_toEndOf="@id/editText_bonus1_editor_valores_3del5"
        app:layout_constraintEnd_toStartOf="@id/editText_bonus2_editor_valores_3del5"
        />
    <TextView
        android:id="@+id/textView_sinal_soma_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:textSize="@dimen/fonte_tableLayout_atributos_nome_atributos"
        android:text="+"
        app:layout_constraintTop_toTopOf="@id/editText_soma_editor_valores_3del5"
        app:layout_constraintBottom_toBottomOf="@id/editText_soma_editor_valores_3del5"
        app:layout_constraintStart_toEndOf="@id/editText_bonus2_editor_valores_3del5"
        app:layout_constraintEnd_toStartOf="@id/editText_bonus3_editor_valores_3del5"
