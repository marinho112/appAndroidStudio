        android:layout_width="@dimen/editText_editor_valores_tamanho"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/bonusUmPersonagem3del5"
        android:ems="10"
        android:inputType="numberSigned"
        android:maxLength="5"
        android:textColor="@color/bonusUmPersonagem3del5"
        app:layout_constraintBottom_toTopOf="@id/btn_salvar_editor_valores_3del5"
        app:layout_constraintEnd_toStartOf="@id/editText_bonus2_editor_valores_3del5"
        app:layout_constraintStart_toEndOf="@id/editText_valor_editor_valores_3del5"
        app:layout_constraintTop_toBottomOf="@id/textFild_total_editor_valores_3del5" />

    <EditText
        android:id="@+id/editText_bonus2_editor_valores_3del5"
        android:layout_width="@dimen/editText_editor_valores_tamanho"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/bonusDoisPersonagem3del5"
        android:ems="10"
        android:inputType="numberSigned"
        android:maxLength="5"
        android:textColor="@color/bonusDoisPersonagem3del5"
        app:layout_constraintBottom_toTopOf="@id/btn_salvar_editor_valores_3del5"
        app:layout_constraintEnd_toStartOf="@id/editText_bonus3_editor_valores_3del5"
        app:layout_constraintStart_toEndOf="@id/editText_bonus1_editor_valores_3del5"
        app:layout_constraintTop_toBottomOf="@id/textFild_total_editor_valores_3del5" />

    <EditText
