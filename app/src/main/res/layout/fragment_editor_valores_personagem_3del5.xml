        app:layout_constraintEnd_toEndOf="@+id/editText_soma_editor_valores_3del5"
        app:layout_constraintStart_toStartOf="@+id/editText_soma_editor_valores_3del5"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_texto_editor_valores_3del5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginLeft="18dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:textSize="@dimen/fonte_tableLayout_atributos_nome_atributos"
        android:text="Determine o seu BÃ´nus Total"
