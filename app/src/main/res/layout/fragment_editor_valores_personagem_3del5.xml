        android:textSize="@dimen/fonte_tableLayout_atributos_nome_atributos"
        android:text="+"
        app:layout_constraintTop_toTopOf="@id/editText_soma_editor_valores_3del5"
        app:layout_constraintBottom_toBottomOf="@id/editText_soma_editor_valores_3del5"
