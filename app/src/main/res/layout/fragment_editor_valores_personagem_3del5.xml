        android:enabled="true"
        android:focusable="false"
        android:inputType="numberSigned"
        app:layout_constraintBottom_toTopOf="@id/btn_salvar_editor_valores_3del5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/editText_bonus4_editor_valores_3del5"
        app:layout_constraintTop_toBottomOf="@id/textFild_total_editor_valores_3del5" />

    <TextView
        android:id="@+id/textFild_total_editor_valores_3del5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
