        android:layout_width="@dimen/editText_editor_valores_tamanho"
        android:layout_height="wrap_content"
        android:ems="10"
        android:enabled="true"
        android:focusable="false"
        android:inputType="numberSigned"
        android:textSize="@dimen/textSize_editor_personagem_3del5"
        app:layout_constraintBottom_toTopOf="@id/btn_salvar_editor_valores_3del5"
        app:layout_constraintEnd_toStartOf="@id/editText_bonus1_editor_valores_3del5"
        app:layout_constraintStart_toStartOf="parent"
